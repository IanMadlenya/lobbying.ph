<div class="detail container">
  <ul class="lobbyists">
    <li class="lobbyist">
      <h2><%= @data.name %></h2>
      <ul>
        <li class="address">
          <%= address_string @data %>
        </li>
        <li class="phone">
          <%= @data.phone %>
        </li>
        <li class="email">
          <%= link_to "mailto:#{@data.email}", @data.email %>
        </li>
      </ul>
    </li>
  </ul>
</div>
